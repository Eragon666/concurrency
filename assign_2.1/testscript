#!/bin/bash

echo 1000 500000 1,2,4,8
prun -v -np 1 assign2_1 1000 500000 1
prun -v -np 1 assign2_1 1000 500000 2
prun -v -np 1 assign2_1 1000 500000 4
prun -v -np 1 assign2_1 1000 500000 6
prun -v -np 1 assign2_1 1000 500000 8
echo

echo 10000 250000 1,2,4,6,8
prun -v -np 1 assign2_1 10000 250000 1
prun -v -np 1 assign2_1 10000 250000 2
prun -v -np 1 assign2_1 10000 250000 4
prun -v -np 1 assign2_1 10000 250000 6
prun -v -np 1 assign2_1 10000 250000 8
echo

echo 100000 100000 1,2,4,6,8
prun -v -np 1 assign2_1 100000 100000 1
prun -v -np 1 assign2_1 100000 100000 2
prun -v -np 1 assign2_1 100000 100000 4
prun -v -np 1 assign2_1 100000 100000 6
prun -v -np 1 assign2_1 100000 100000 8
echo

echo 1000000 20000 1,2,4,6,8
prun -v -np 1 assign2_1 1000000 20000 1
prun -v -np 1 assign2_1 1000000 20000 2
prun -v -np 1 assign2_1 1000000 20000 4
prun -v -np 1 assign2_1 1000000 20000 6
prun -v -np 1 assign2_1 1000000 20000 8
echo

echo 10000000 3500 1,2,4,6,8
prun -v -np 1 assign2_1 10000000 3500 1
prun -v -np 1 assign2_1 10000000 3500 2
prun -v -np 1 assign2_1 10000000 3500 4
prun -v -np 1 assign2_1 10000000 3500 6
prun -v -np 1 assign2_1 10000000 3500 8
echo
